<form action="/rnode/<%= host %>/<%= port %>/?fluent-auth-token=<%= fwtoken %>" method="get" target="_blank">
  <input type="hidden" name="fw-auth-token" value="<%= fwtoken %>"/>
  <button class="btn btn-primary" style="background-color:#ffb71c; border-color: black; color: black" type="submit">
    <i class="fa-solid fa-mound"></i> Connect
  </button>
</form>